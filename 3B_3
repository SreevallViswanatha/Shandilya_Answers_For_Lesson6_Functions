// Create a function that takes an array of 10 integers and returns the sum of the array.
#include <stdio.h>
#include <stdlib.h>
int sum(int []);
int main()
{
    int a[10], i;
    for(i=1; i<=10; i++)
    {
        printf("Enter a number:");
        scanf("%d", &a[i-1]);
    }
    printf("The sum is:%d", sum(a));
}
int sum(int a[10])
{
    int i=0, sum=0;
    for(; i<=9; i++)
    {
        sum+=a[i];
    }
    return sum;
}
