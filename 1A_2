// Write a program in C to find the square of any number using the function.
#include <stdio.h>
#include <stdlib.h>
float square(float);
int main()
{
    float number;
    printf("Enter a number:");
    scanf("%f", &number);
    printf("The square of %.2f is:%.2f", number, square(number));
}
float square(float number)
{
    return(number*number);
}
